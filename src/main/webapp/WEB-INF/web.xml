<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>twitter4j-demo</display-name>
  
  <servlet>
  	<servlet-name>Auth</servlet-name>
  	<servlet-class>twitter4j.demo.Auth</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AuthCallback</servlet-name>
  	<servlet-class>twitter4j.demo.AuthCallback</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PrivateMessages</servlet-name>
  	<servlet-class>twitter4j.demo.PrivateMessages</servlet-class>
  </servlet>
  
  <servlet-mapping>
	<servlet-name>Auth</servlet-name>
	<url-pattern>/Auth</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>AuthCallback</servlet-name>
	<url-pattern>/AuthCallback</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>PrivateMessages</servlet-name>
	<url-pattern>/PrivateMessages</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
